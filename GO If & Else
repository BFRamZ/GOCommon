package main

import (
"fmt"
)

func main() {
    
    if 9%2 == 0 {
        fmt.Println("9 is even")
    } else {
        fmt.Println("9 is odd")
    }
	

    if 16%8 == 0 {
        fmt.Println("16 is divisible by 8")
    }
        
    if num := 5; num < 0 {
        fmt.Println(num, "is negative")
    } else if num < 10 {
        fmt.Println(num, "has 1 digit")
    } else {
        fmt.Println(num, "has multiple digits")
    }
}
